{
    "dashboard": {
      "title": "Churn Prediction API Metrics",
      "tags": ["api", "mlops"],
      "timezone": "browser",
      "panels": [
        {
          "id": 1,
          "title": "Prediction Request Rate",
          "type": "graph",
          "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
          "targets": [
            {
              "expr": "rate(prediction_requests_total[5m])",
              "legendFormat": "{{prediction_label}}"
            }
          ]
        },
        {
          "id": 2,
          "title": "API Latency (p95)",
          "type": "graph",
          "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
          "targets": [
            {
              "expr": "histogram_quantile(0.95, rate(prediction_latency_seconds_bucket[5m]))"
            }
          ]
        },
        {
          "id": 3,
          "title": "Prediction Distribution",
          "type": "piechart",
          "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
          "targets": [
            {
              "expr": "prediction_requests_total",
              "legendFormat": "{{prediction_label}}"
            }
          ]
        },
        {
          "id": 4,
          "title": "Churn Probability Distribution",
          "type": "heatmap",
          "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
          "targets": [
            {
              "expr": "rate(prediction_probability_bucket[5m])"
            }
          ]
        }
      ]
    }
  }